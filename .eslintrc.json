{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        // "max-len": ["error", { "code": 80, "tabWidth": 4 }, { "ignoreComments": true }]
        "no-console": ["error", { "allow": ["log"] }],
        "no-param-reassign": ["error", { "props": false }],
        "no-undef": ["error", { "test": true}]
    }
}
